<!doctype html>
<html>
  <head>
    <title>Users at <%=shopData.shopName%></title>
    <link rel='stylesheet'  type='text/css' href='/main.css' />
  </head>
  <body>
    <h1><%=shopData.shopName%> Users</h1>
    <ul>
      <% users.forEach(function(user) { %>
        <li><%= user.username %>, <%= user.firstname%> <%= user.lastname%>, <%= user.email %></li>
      <% }) %>
    </ul>
    </body>
</html>